{
	"name": "SplendidCRM",
	"version": "15.0.8828",
	"description": "SplendidCRM Mobile React Client",
	"author": "Paul Rony",
	"scripts": {
		"watch": "webpack --watch --config=configs/webpack/prod.js",
		"build": "webpack --config=configs/webpack/prod.js",
		"build:min": "webpack --config=configs/webpack/prod_minimize.js",
		"build:cordova": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js --config=configs/webpack/mobile.js",
		"build:android": "cordova build android",
		"build:androidRelease": "cordova build android --release --buildConfig=build.json",
		"build:ios": "cordova build ios",
		"configure:cordova": "cordova prepare",
		"configure:android": "cordova platform add android",
		"configure:ios": "cordova platform add ios",
		"start": "npm run dev-local",
		"start:android": "cordova run android --device",
		"start:ios": "cordova run ios --device",
		"dev-local": "webpack-dev-server --env.PATH=/SplendidCRM/ --config=configs/webpack/dev_local.js",
		"dev-remote": "webpack-dev-server --env.PATH=/SplendidCRM/ --config=configs/webpack/dev_remote.js"
	},
